    /* 弹窗样式 */
    .address_info{
        color: white;
        padding-left: 20px;
        overflow:auto;
        width: 100%;
        height: 100%;
    }
    .address_info p{
        line-height: 30px;
        height: 30px;
        font-size: 15px;
    }
    
    ::-webkit-scrollbar-thumb {/*滚动条里面小方块*/
        border-radius:10px;
        width: 20px;
        background: rgba(8, 17, 42, 0.8);
         }
    ::-webkit-scrollbar{
         width: 20px;
        background-color: rgba(8, 17, 42, 0.2);
        margin-right:20px; 
        border-radius:50%;
    }
    ::-webkit-scrollbar-track {/*滚动条里面轨道*/
    border-radius: 10px;
    width: 20px;
    background: rgba(8, 17, 42, 0.5);
}
